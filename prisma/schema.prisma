generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider          = "postgresql"
  url               = env("POSTGRES_PRISMA_URL")
  directUrl         = env("POSTGRES_URL_NON_POOLING")
  shadowDatabaseUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model Bandwidth {
  id                                    Int      @id @default(autoincrement())
  time                                  DateTime @unique
  server_id                             Int      @unique
  app_id                                Int      @unique
  servername                            String
  appname                               String
  TotalBandwidthByCountry               BigInt
  TotalBandwidthByIsp                   BigInt
  TotalBandwidthReceiveClientHTTP       BigInt
  TotalBandwidthReceiveClientHTTP2      BigInt
  TotalBandwidthReceiveClientHTTPS      BigInt
  TotalBandwidthReceiveClientInternal   BigInt
  TotalBandwidthReceiveDownload         BigInt
  TotalBandwidthReceiveDownloadInternal BigInt
  TotalBandwidthSendClientHTTP          BigInt
  TotalBandwidthSendClientHTTP2         BigInt
  TotalBandwidthSendClientHTTPS         BigInt
  TotalBandwidthSendClientInternal      BigInt
  TotalBandwidthSendDownload            BigInt
  TotalBandwidthSendDownloadInternal    BigInt
}
